{
  "name": "@medv/numbr",
  "version": "1.0.0",
  "description": "A smart calculator",
  "main": "index.js",
  "scripts": {
    "test": "ava",
    "serve": "npm run build:runtime && node -r ts-node/register server.ts",
    "build:runtime": "node scripts/build-runtime.js --outdir public",
    "build:static": "node scripts/build-static.js"
  },
  "repository": "antonmedv/numbr",
  "keywords": [
    "calculator"
  ],
  "author": "Anton Medvedev <anton@medv.io>",
  "license": "MIT",
  "homepage": "https://numbr.dev",
  "devDependencies": {
    "ava": "^4.0.1",
    "esbuild": "^0.21.5"
  },
  "ava": {
    "files": [
      "**/*.test.ts"
    ],
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "dependencies": {
    "@types/big.js": "^6.1.2",
    "big.js": "^6.1.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  }
}
